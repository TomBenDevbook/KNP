broadcast_buffer = buffer_create( 256, buffer_fixed, 1);   
buffer_seek(broadcast_buffer, buffer_seek_start, 0);
buffer_write(broadcast_buffer, buffer_string, powerword);
network_send_broadcast(server, port, broadcast_buffer, buffer_tell(broadcast_buffer));
write_log('Powerword: '+powerword); 
 
 
