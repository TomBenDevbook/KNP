//establish udp server
write_log('Trying to establish udp server.');
port = 49090; //start with this port number 

  server = network_create_server(network_socket_udp, port, 100);
 while (server &lt; 0 &amp;&amp; port &lt; 65535)
    {
    port++
    server = network_create_server(network_socket_udp, port, 100);
    }    
if port&gt;=65535 { write_log('Fail to establish udp server. No response on any port..');  game_end(); } 
else
{
write_log('UDP server '+string(server)+' created at port '+string(port)+'.');
}
  

